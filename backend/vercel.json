{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "env": {
    "PORT": "3001",
    "NODE_ENV": "production",
    "FRONTEND_URL": "@frontend-url",
    "PINATA_API_KEY": "@pinata-key",
    "CONTRACT_ADDRESS": "@contract-address"
  }
}
