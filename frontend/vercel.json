{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_BACKEND_URL": "@backend-url",
    "VITE_RPC_URL": "@rpc-url",
    "VITE_CONTRACT_ADDRESS": "@contract-address",
    "VITE_CHAIN_ID": "@chain-id",
    "VITE_CHAIN_NAME": "@chain-name"
  }
}
